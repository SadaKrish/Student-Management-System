@model IEnumerable<sms.Models.Teacher>

@{
    ViewBag.Title = "StudentTeachers";
}

<h2>Teachers for Student</h2>

<table class="table">
    <tr>
        <th>Teacher ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Gender</th>
        <th>Date of Birth</th>
        <!-- Add more columns as needed -->
    </tr>

    @foreach (var teacher in Model)
    {
        <tr>
            <td>@teacher.TeacherID</td>
            <td>@teacher.FirstName</td>
            <td>@teacher.LastName</td>
            <td>@teacher.Gender</td>
            <td>@teacher.DOB</td>
            <!-- Add more columns as needed -->
        </tr>
    }
</table>



